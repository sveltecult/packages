{
	"name": "@sveltecult/formdata-validator",
	"author": "Sveltecult <leader@sveltecult.com>",
	"description": "A versatile FormData validation library offering a fluent API to ensure data integrity and consistency in web applications.",
  	"keywords": [
		"form-data",
		"validator",
		"validation",
		"form-validation",
		"form-validator"
	],
	"repository": {
    	"type": "git",
    	"url": "git+https://github.com/sveltecult/packages.git#formdata-validator"
  	},
  	"bugs": {
    	"url": "https://github.com/sveltecult/formdata-validator/issues"
  	},
  	"homepage": "https://formdata-validator.sveltecult.com",
	"license": "MIT",
	"version": "0.0.4",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"file-type": "^18.7.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.30.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"file-type": "^18.7.0"
	}
}
